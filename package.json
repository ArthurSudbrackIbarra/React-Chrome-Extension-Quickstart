{
  "name": "react-chrome-extension-quickstart",
  "version": "1.0.4",
  "description": "A npm package to generate a quickstart template for creating Chrome extensions.",
  "type": "module",
  "bin": {
    "react-chrome-extension-quickstart": "./dist/cli/entrypoint.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@types/node": "16.18.3",
    "fs-extra": "11.1.0",
    "prompt-sync": "4.2.0",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "npx tsc && xcopy src\\template-files dist\\template-files\\ /E /H /Y /exclude:.copyignore",
    "test-package-locally": "npm link --force && npx react-extension-quickstart create test-name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurSudbrackIbarra/React-Chrome-Extension-Quickstart"
  },
  "keywords": [
    "chrome",
    "chrome-extension",
    "browser-extension",
    "react",
    "typescript",
    "quickstart",
    "template"
  ],
  "author": "Arthur Sudbrack Ibarra",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ArthurSudbrackIbarra/React-Chrome-Extension-Quickstart/issues"
  },
  "homepage": "https://github.com/ArthurSudbrackIbarra/React-Chrome-Extension-Quickstart#readme",
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/prompt-sync": "4.2.0"
  }
}
