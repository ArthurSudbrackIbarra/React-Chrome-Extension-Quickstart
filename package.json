{
  "name": "react-extension-quickstart",
  "version": "1.0.0",
  "description": "npm package to generate a quickstart template for creating Chrome extensions.",
  "type": "module",
  "bin": {
    "react-extension-quickstart": "./dist/cli/entrypoint.js"
  },
  "dependencies": {
    "@types/node": "16.18.3",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "npx tsc && xcopy src\\template-files dist\\template-files\\ /E /H /Y /exclude:.excludefiles",
